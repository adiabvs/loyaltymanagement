{
  "name": "loyalty-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export",
    "build:android": "eas build --platform android --profile production",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:local": "eas build --platform android --profile production --local",
    "prebuild:android": "expo prebuild --platform android --clean",
    "build:apk": "cross-env NODE_ENV=production EXPO_PUBLIC_ENV=production EXPO_PUBLIC_API_URL=https://loyaltymanagement.onrender.com/api expo prebuild --platform android --clean && cd android && ./gradlew assembleRelease && cd ..",
    "build:apk:windows": "cross-env NODE_ENV=production EXPO_PUBLIC_ENV=production EXPO_PUBLIC_API_URL=https://loyaltymanagement.onrender.com/api expo prebuild --platform android --clean && cd android && gradlew.bat assembleRelease && cd .."
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "expo": "^54.0.26",
    "expo-asset": "~11.0.5",
    "expo-barcode-scanner": "^13.0.0",
    "expo-constants": "^16.0.0",
    "expo-status-bar": "^2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-qrcode-svg": "^6.3.2",
    "react-native-reanimated": "^3.16.0",
    "react-native-safe-area-context": "^4.10.0",
    "react-native-screens": "^4.9.0",
    "react-native-web": "~0.19.13",
    "expo-build-properties": "^1.0.10"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "cross-env": "^7.0.3",
    "eas-cli": "^7.8.5",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  }
}
